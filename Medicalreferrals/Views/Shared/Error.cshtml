@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">An error occurred while processing your request.</h2>

<table class="table table-striped">
    <tr>
        <td>@Html.DisplayNameFor(model => model.ActionName)</td>
        <td>@Html.DisplayFor(model => model.ActionName)</td>
    </tr>
    <tr>
        <td>@Html.DisplayNameFor(model => model.ControllerName)</td>
        <td>@Html.DisplayFor(model => model.ControllerName)</td>
    </tr>
    <tr>
        <td>@Html.DisplayNameFor(model => model.Exception)</td>
        <td>@Html.DisplayFor(model => model.Exception)</td>
    </tr>
</table>

